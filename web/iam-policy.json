{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Action":[
        "s3:GetLifecycleConfiguration",
        "s3:ListBucket"
      ],
      "Resource":[
        "arn:aws:s3:::hk-deploy"
      ]
    },
    {
      "Effect":"Allow",
      "Action":[
        "s3:GetLifecycleConfiguration",
        "s3:GetObject",
        "s3:GetObjectAcl",
        "s3:ListBucket",
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource":[
        "arn:aws:s3:::hk-deploy/*"
      ]
    }
  ]
}
